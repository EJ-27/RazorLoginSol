@page
@model RazorLogin.Pages.Admin.GShps.IndexModel

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-page="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.GiftShop[0].ShopId)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.GiftShop[0].GiftShopName)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.GiftShop[0].GiftShopLocation)
            </th>
         
            <th>
                @Html.DisplayNameFor(model => model.GiftShop[0].GiftShopOpenTime)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.GiftShop[0].GiftShopCloseTime)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.GiftShop[0].GiftShopYearToDateSales)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model.GiftShop) {
        <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.ShopId)
                </td>
            <td>
                @Html.DisplayFor(modelItem => item.GiftShopName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.GiftShopLocation)
            </td>
          
            <td>
                @Html.DisplayFor(modelItem => item.GiftShopOpenTime)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.GiftShopCloseTime)
            </td>
                <td>
                    @Html.DisplayFor(modelItem => item.GiftShopYearToDateSales)
                </td>
            <td>
                <a asp-page="./Edit" asp-route-id="@item.ShopId">Edit</a> |
                <a asp-page="./Delete" asp-route-id="@item.ShopId">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
